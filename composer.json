{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "axlon/laravel-postal-code-validation": "^3.3",
        "biscolab/laravel-recaptcha": "^6.0",
        "blade-ui-kit/blade-icons": "^1.5",
        "doctrine/dbal": "^3.0",
        "geoip2/geoip2": "~2.0",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.2",
        "little-apps/littlejwt": "^1.3",
        "owenvoke/blade-fontawesome": "^2.4",
        "paquettg/php-html-parser": "^3.1",
        "pelmered/fake-car": "^1.7",
        "spatie/commonmark-shiki-highlighter": "^2.1"
    },
    "require-dev": {
        "davidbadura/faker-markdown-generator": "^1.1",
        "fakerphp/faker": "^1.9.1",
        "fossbarrow/laravel-phpcs": "dev-main",
        "friendsofphp/php-cs-fixer": "^3.9",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.8",
        "phpunit/phpunit": "^10.3.2",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
