{
    "name": "sameoldnick/sameoldwebsite",
    "type": "project",
    "description": "Here, you'll find the code that powers Nick's personal corner of the internet.",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "axlon/laravel-postal-code-validation": "^3.3",
        "biscolab/laravel-recaptcha": "^6.0",
        "blade-ui-kit/blade-icons": "^1.5",
        "composer/composer": "^2.6",
        "geoip2/geoip2": "~3.0",
        "google/analytics-data": "^0.11.1",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.9",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.2",
        "little-apps/littlejwt": "^2.1.0@beta",
        "owenvoke/blade-fontawesome": "^2.4",
        "paquettg/php-html-parser": "^3.1",
        "pelmered/fake-car": "^2.0",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/commonmark-shiki-highlighter": "^2.1",
        "spatie/url": "^2.2",
        "spomky-labs/otphp": "^11.2",
        "starkbank/ecdsa": "^2.1",
        "symfony/process": "^7.0",
        "thecodingmachine/safe": "^2.5"
    },
    "require-dev": {
        "davidbadura/faker-markdown-generator": "^1.1",
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.15",
        "laravel/sail": "^1.27",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.1.3",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
